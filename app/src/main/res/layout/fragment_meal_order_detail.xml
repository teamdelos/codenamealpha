<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:orientation="vertical"
    tools:context="com.cmu.delos.codenamealpha.ui.consumer.MealOrderDetailActivityFragment">
    <ImageButton
        android:layout_width="match_parent"
        android:layout_gravity="center_horizontal"
        android:layout_height="264dp"
        android:src="@drawable/provider2_meal6"/>
    <ImageButton
        android:layout_width="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_height="wrap_content"
        android:src="@drawable/provider2_meal2"
        android:adjustViewBounds="false"
        android:baselineAlignBottom="false"/>
    <ImageButton
        android:layout_width="386dp"
        android:layout_gravity="center_horizontal"
        android:layout_height="match_parent"
        android:src="@drawable/provider2_meal3"/>
    <ImageButton
        android:layout_width="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_height="wrap_content"
        android:src="@drawable/provider2_meal4"
        android:adjustViewBounds="false"
        android:baselineAlignBottom="false"/>
    <ImageButton
        android:layout_width="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_height="wrap_content"
        android:src="@drawable/provider2_meal5"/>
    <ImageButton
        android:layout_width="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_height="wrap_content"
        android:src="@drawable/provider2_meal1"
        android:adjustViewBounds="false"
        android:baselineAlignBottom="false"/>
    <ImageButton
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:id="@+id/imageBtn1"
        android:clickable="true"
        android:src="@drawable/provider2_meal5"
        android:layout_gravity="center_horizontal"
        android:adjustViewBounds="false"
        android:paddingBottom="50dp"
        android:elevation="@dimen/snackbar_elevation" />
    <ImageButton
        android:layout_width="match_parent"
        android:paddingTop="50dp"
        android:layout_height="100dp"
        android:id="@+id/imageBtn2"
        android:clickable="true"
        android:src="@drawable/provider2_meal4"
        android:layout_gravity="center_horizontal"
        android:adjustViewBounds="false"
        android:elevation="@dimen/snackbar_elevation" />


</LinearLayout>
